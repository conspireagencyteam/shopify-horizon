{%- doc -%}
  Intended for use in a block similar to the text block.

  @param {object} [item] - item object
{%- enddoc -%}


{% assign icon_block_class = 'icon-block__media icon-block-' | append: block.id %}

{% capture icon %}
  {% render 'icon-or-image',
    icon: 'none',
    image_upload: item.icon.value,
    width: block.settings.icon_width,
    class_name: icon_block_class,
    attributes: block.shopify_attributes
  %}
{% endcapture %}

{%  capture class %}
  group-block
  group-block--height-{{ block.settings.height }}
  group-block--width-{{ block.settings.width }}
  {% if block.settings.use_individual_borders %}border-style--individual{% else %}border-style{% endif %}
  spacing-style
  1-spacing-style
  size-style
  {% if block.settings.position != 'static' %}position-{{ block.settings.position }}{% endif %}
  {% if block.settings.enable_parallax %}parallax-container parallax-{{ block.settings.parallax_speed }} parallax-{{ block.settings.parallax_direction }}{% unless block.settings.parallax_on_mobile %} parallax-desktop-only{% endunless %}{% endif %}
  {% if block.settings.inherit_color_scheme == false %} color-{{ block.settings.color_scheme }}{% endif %}
  {{ class }}
{% endcapture %}
{% capture style %}
  {% if block.settings.use_individual_borders %}
    {% render '1-border-override', settings: block.settings %}
  {% else %}
    {% render 'border-override', settings: block.settings %}
  {% endif %}
  {% render '1-spacing-style', settings: block.settings %}
  {% render 'size-style', settings: block.settings %}
  {% if block.settings.position != 'static' %}
    position: {{ block.settings.position }};
    {% if block.settings.z_index != 0 %}z-index: {{ block.settings.z_index }};{% endif %}
    {% if block.settings.position == 'absolute' %}
      {% if block.settings.position_top != 0 %}top: {{ block.settings.position_top }}%;{% endif %}
      {% if block.settings.position_bottom != 0 %}bottom: {{ block.settings.position_bottom }}%;{% endif %}
      {% if block.settings.position_left != 0 %}left: {{ block.settings.position_left }}%;{% endif %}
      {% if block.settings.position_right != 0 %}right: {{ block.settings.position_right }}%;{% endif %}
    {% endif %}
  {% endif %}
  {{ style }}
{% endcapture %}
{% capture content %}
  {%- if block.settings.link != blank -%}
    <a
      href="{{ block.settings.link }}"
      class="group-block__link"
      {% if block.settings.open_in_new_tab %}
        target="_blank" rel="noopener"
      {% endif %}
    ></a>
  {%- endif -%}

  <div class="group-block__media-wrapper">
    {% render 'background-media',
      background_media: block.settings.background_media,
      background_video: block.settings.video,
      background_video_position: block.settings.video_position,
      background_image: block.settings.background_image,
      background_image_position: block.settings.background_image_position
    %}
    {% if block.settings.toggle_overlay %}
      {% render 'overlay', settings: block.settings, layer: '0' %}
    {% endif %}
  </div>

  <div
    class="
      group-block-content
      {% if request.design_mode %}group-block-content--design-mode{% endif %}
      layout-panel-flex
      1-layout-panel-flex
      layout-panel-flex--{{ block.settings.content_direction | default: 'column' }}
      1-layout-panel-flex--{{ block.settings.content_direction | default: 'column' }}
      {% if block.settings.content_direction == 'row' and block.settings.vertical_on_mobile %} 1-mobile-column mobile-column{% endif %}
      {% if block.settings.content_direction == 'column' and block.settings.horizontal_on_mobile %} 1-mobile-row mobile-row{% endif %}
    "
    style="    
      {% render '1-layout-panel-style', settings: block.settings %}
    "
  >
    {{ icon }}
  </div>
{% endcapture %}
{% if block.settings.enable_parallax %}
<parallax-group
  class="{{ class }}"
  style="{{ style }}"
  data-parallax="true"
  data-parallax-speed="{{ block.settings.parallax_speed }}"
  data-parallax-direction="{{ block.settings.parallax_direction }}"
  {{ block.shopify_attributes }}
  data-testid="group-block"
>
  {{- content -}}
</parallax-group>
{% else %}
<div
  class="{{ class }}"
  style="{{ style }}"
  {{ block.shopify_attributes }}
  data-testid="group-block"
>
  {{- content -}}
</div>
{% endif %}
{% stylesheet %}
  .icon-block {
    display: flex;
    fill: currentcolor;
    flex-shrink: 0;
  }

  .icon-block__media {
    height: auto;
  }
{% endstylesheet %}

