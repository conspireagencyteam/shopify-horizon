{% capture children %}
  {%- content_for 'block', type: '_content', id: 'static-header' -%}
{% endcapture %}

{% capture list_items %}
  {% for health_interest in section.settings.health_interests %}
    <div class="resource-list__item group-block resource-card">
      <a
        href="{{ routes.all_products_collection_url }}?filter.p.m.product_details.primary_health_interest=gid%3A%2F%2Fshopify%2FMetaobject%2F{{ health_interest.system.id }}"
        class="group-block__link"
        {% if settings.open_in_new_tab %}
          target="_blank" rel="noopener"
        {% endif %}
      ></a>
      <div class="group-block-content">
        {% content_for 'block', type: '_1-health-interest', id: 'static-health-interest', closest.metaobject.health_interests: health_interest %}
      </div>
    </div>
    <!--@list/split-->
  {% endfor %}
{% endcapture %}

{% capture footer %}
  {%- content_for 'block', type: '_content', id: 'static-footer' -%}
{% endcapture %}

{% render '1-health-interests', children: children, list_items: list_items, settings: section.settings, footer: footer%}

