{% liquid
  assign max_items = section.settings.max_products
%}
{% liquid
  case section.settings.layout_type
    when 'grid'
      assign classes = 'resource-list--grid'
    when 'carousel'
      assign classes = 'resource-list__carousel'
  endcase

  capture styles
    echo '--resource-list-column-gap-desktop: ' | append: section.settings.columns_gap | append: 'px;'
    
    if section.settings.layout_type == 'grid'
      echo '--resource-list-row-gap-desktop: ' | append: section.settings.rows_gap | append: 'px;'
      echo '--resource-list-columns: repeat(' | append: section.settings.columns | append: ', 1fr);'
    endif
    
    if section.settings.layout_type == 'carousel'
      echo '--column-count: ' | append: section.settings.columns | append: ';'
    endif
    
    if section.settings.layout_type_mobile == 'grid'
      echo '--resource-list-columns-mobile: repeat(' | append: section.settings.mobile_columns | append: ', 1fr);'
    endif
  endcapture

  if section.settings.layout_type == 'carousel' and section.settings.slideshow_controls_style == 'tabs' or section.settings.slideshow_mobile_controls_style == 'tabs'
    assign tablist = section.settings.products | map: 'title' | slice: 0, max_items
    assign controls_position = 'top'
  endif
%}

<div class="section-background color-{{ section.settings.color_scheme }}"></div>
<div
  class="
    section
    section--{{ section.settings.section_width }}
    color-{{ section.settings.color_scheme }}
    section-resource-list
    spacing-style
    gap-style
  "
  style="
    --column-count-mobile: {{ section.settings.mobile_columns }};
    {% render 'spacing-style', settings: section.settings %}
    {% render 'gap-style', value: section.settings.gap %}
    {{ styles }}
  "
>
  <div class="section-resource-list__header">
    {%- content_for 'block', type: '_content', id: 'static-header' -%}
  </div>

  {% capture list_items %}
    {% if section.settings.products.count > 0 and shop.products_count != 0 %}
      {% for product in section.settings.products limit: max_items %}
        {% content_for 'block', type: '_1-list-item-featured-product', id: 'list-item-featured-product', closest.product: product %}

        {% unless forloop.last %}
          <!--@list/split-->
        {% endunless %}
      {% endfor %}
    {% else %}

      {% for i in (1..max_items) %}
        <div
          class="resource-list__item"
        >
          {% # theme-check-disable UniqueStaticBlockId %}
            {% content_for 'block', type: '_1-list-item-featured-product', id: 'list-item-featured-product', closest.product: null %}
          {% # theme-check-enable UniqueStaticBlockId %}
        </div>

        {% unless forloop.last %}
          <!--@list/split-->
        {% endunless %}
      {% endfor %}
    {% endif %}
  {% endcapture %}

  {% liquid
    # Create an array from the list items to be used for different layout types
    assign list_items_array = list_items | strip | split: '<!--@list/split-->'
  %}
  

  <div
    class="
      1-list-featured-product
      resource-list
      {% if section.settings.layout_type == 'carousel' %}
        force-full-width
      {% endif %}
      {% if section.settings.layout_type_mobile != section.settings.layout_type %}
        hidden--mobile
      {% endif %}
      {{ classes }}
    "
    style="
      {{ styles }}
    "
    {% if section.settings.layout_type == 'grid' %}
      data-testid="resource-list-grid"
    {% endif %}
  >
    {% case section.settings.layout_type %}
      {% when 'grid' %}
        {{ list_items }}
      {% when 'carousel' %}
        {% render '1-list-carousel',
          ref: 'resourceListFeaturedProductCarousel',
          slides: list_items_array,
          slide_count: section.settings.products.count,
          settings: section.settings,
          tablist: tablist,
          controls_position: controls_position
        %}
    {% endcase %}
  </div>

  {% if section.settings.layout_type_mobile != section.settings.layout_type %}
    {% case section.settings.layout_type_mobile %}
      {% when 'carousel' %}
        {% liquid
          assign mobile_carousel_gap = section.settings.columns_gap
        %}
        <div
          class="
            resource-list
            hidden--desktop
            force-full-width
          "
          style="
            --resource-list-column-gap-desktop: {{ mobile_carousel_gap }}px;
            --column-count: {{ section.settings.columns }};
          "
        >
          {% render '1-list-carousel',
            ref: 'resourceListFeaturedProductCarouselMobile',
            slides: list_items_array,
            slide_count: section.settings.products.count,
            settings: section.settings,
            tablist: tablist
          %}
        </div>
      {% when 'grid' %}
        <div
          class="
            resource-list
            hidden--desktop
            resource-list--grid
          "
          style="
            --resource-list-column-gap-desktop: {{ section.settings.columns_gap }}px;
            --resource-list-row-gap-desktop: {{ section.settings.rows_gap }}px;
            --resource-list-columns: repeat({{ section.settings.columns }}, 1fr);
            --resource-list-columns-mobile: repeat({{ section.settings.mobile_columns }}, 1fr);
          "
        >
          {{ list_items }}
        </div>
    {% endcase %}
  {% endif %}
  <div class="section-resource-list__footer">
    {%- content_for 'block', type: '_content', id: 'static-footer' -%}
  </div>
</div>

{% stylesheet %}
  .\31-list-featured-product .resource-list__slide {
    /* transition: opacity 0.2s ease; */
  }

  .\31-list-featured-product:not([transitioning]) .resource-list__slide[aria-hidden="true"] {
    /* opacity: 0; */
  }

  .\31 -slideshow-controls__tablist.slideshow-controls__thumbnails {
    background: rgb(var(--color-border-rgb) / 0.04);
    border-radius: var(--style-border-radius-inputs);
    padding: var(--padding-2xs);
    margin-bottom: var(--margin-4xl);
    @media screen and (max-width: 449px) {
      width: var(--full-page-grid-central-column-width);
    }
  }

  .\31 -slideshow-controls__tab.slideshow-control {
    padding: var(--padding-lg) var(--padding-2xl);
    border-radius: var(--style-border-radius-inputs);
    background: transparent;
    color: var(--color-foreground-heading);
    font-size: var(--font-size--lg);
    line-height: 1;
    @media screen and (max-width: 449px) {
      flex: 1;
    }
  }

  .slideshow-controls__thumbnails .\31 -slideshow-controls__tab.slideshow-control:is([aria-selected='true']) {
    background: var(--color-primary-button-background);
    color: var(--color-primary-button-text);
    outline: none;
  }
{% endstylesheet %}

