{
  "name": "conspire-horizon-theme",
  "version": "1.0.0",
  "description": "Conspire Horizon - Modern Shopify Online Store 2.0 Theme",
  "private": true,
  "scripts": {
    "dev": "shopify theme dev --store pluscoffee1 --open",
    "dev:host": "shopify theme dev --store pluscoffee1 --host",
    "push": "shopify theme push --store pluscoffee1",
    "push:dev": "shopify theme push --store pluscoffee1 --development",
    "push:unpublished": "shopify theme push --store pluscoffee1 --unpublished",
    "push:live": "shopify theme push --store pluscoffee1 --live --allow-live",
    "pull": "shopify theme pull --store pluscoffee1",
    "pull:dev": "shopify theme pull --store pluscoffee1 --development",
    "pull:live": "shopify theme pull --store pluscoffee1 --live",
    "check": "shopify theme check",
    "check:auto": "shopify theme check --auto-correct",
    "open": "shopify theme open --store pluscoffee1",
    "open:dev": "shopify theme open --store pluscoffee1 --development",
    "open:editor": "shopify theme open --store pluscoffee1 --editor",
    "list": "shopify theme list --store pluscoffee1",
    "info": "shopify theme info --store pluscoffee1",
    "share": "shopify theme share --store pluscoffee1",
    "package": "shopify theme package",
    "publish": "shopify theme publish --store pluscoffee1",
    "delete": "shopify theme delete --store pluscoffee1",
    "console": "shopify theme console --store pluscoffee1",
    "auth:logout": "shopify auth logout"
  },
  "devDependencies": {
    "@shopify/cli": "latest",
    "@shopify/theme": "latest"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "shopify",
    "theme",
    "shopify-theme",
    "online-store-2.0"
  ]
}